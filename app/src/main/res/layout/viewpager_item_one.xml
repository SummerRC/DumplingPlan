<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="fill_parent"
    android:clickable="false"
    android:focusable="false"
    android:id="@+id/view_item_rootView"
     android:layout_height="fill_parent">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="54dp"
        android:layout_marginLeft="32dp"
        android:id="@+id/viewPager_viewContainer"/>

    <ImageView
        android:id="@+id/iv_eggplant"
        android:layout_above="@+id/iv_tomato"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="56dp"
        android:layout_marginBottom="14dp"
        android:background="@mipmap/eggplant"/>

    <ImageView
        android:id="@+id/iv_cucumber"
        android:layout_toRightOf="@+id/iv_eggplant"
        android:layout_above="@+id/iv_tomato"
        android:layout_marginBottom="14dp"
        android:layout_marginLeft="6dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/cucumber_lock"/>

    <ImageView
        android:id="@+id/iv_pimiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/iv_cucumber"
        android:layout_above="@+id/iv_tomato"
        android:layout_marginBottom="14dp"
        android:layout_marginLeft="6dp"
        android:background="@mipmap/pimiento_lock"/>

    <ImageView
        android:id="@+id/iv_cabbage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/iv_pimiento"
        android:layout_above="@+id/iv_tomato"
        android:layout_marginBottom="14dp"
       />

    <ImageView
        android:id="@+id/iv_tomato"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="110dp"
        android:layout_marginLeft="60dp"
        android:background="@mipmap/tomato_lock"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@mipmap/word_vegetable"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="50dp"/>

</RelativeLayout>
