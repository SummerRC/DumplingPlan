<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rootView"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@mipmap/soho_background_welcome">


    <ImageView
        android:id="@+id/iv_stock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="210dp"
        android:layout_marginTop="74dp"
        android:background="@mipmap/soho_welcome_stock"/>

    <ImageView
        android:id="@+id/iv_ventilator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="60dp"
        android:layout_alignParentEnd="true"
        android:background="@mipmap/soho_welcome_ventilator"/>

    <ImageView
        android:id="@+id/iv_setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="17dp"
        android:layout_alignParentBottom="true"
        android:background="@mipmap/soho_welcome_btn_setting"/>



</RelativeLayout>
