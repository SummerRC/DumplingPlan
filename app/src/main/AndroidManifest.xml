<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.summerrc.dumplingplan" >

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.activity.SplashActivity"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.WelcomeActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.SelectFoodActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.SelectSeasoningActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.StuffingActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.DoughActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.RubActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.CutActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.SkinActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.AddStuffingActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.PackActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.PutActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.ShakeActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.ScoreActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.FoodDescriptionActivity"
            android:screenOrientation="landscape"
            android:theme="@style/DialogActivity" >
        </activity>
        <activity
            android:name=".ui.activity.BasketActivity"
            android:theme="@style/DialogActivity" >
        </activity>
        <activity
            android:name=".ui.activity.LockActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".ui.activity.SettingActivity"
            android:theme="@style/DialogActivity" >
        </activity>
        <activity
            android:name=".interestinggame.lianliankan.LianliankanActivity"
            android:configChanges="keyboardHidden|orientation"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name=".interestinggame.cutfood.CutFoodActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".interestinggame.selectmore.SelectMoreActivity"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name=".interestinggame.SkinePackStuffing.SkinPackStuffingActivity"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name=".ui.activity.AboutUsActivity"
            android:screenOrientation="landscape" >
        </activity>
    </application>

</manifest>
